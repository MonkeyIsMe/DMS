<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
          

<!-- package属性：填写一个包名 -->
<hibernate-mapping>
<class name="com.dms.model.Information_notice" table="Information_notice">

   <!-- id用来配置主键属性 column表示表中的主键列名-->
   <id name="infor_id" column="infor_id">
   <generator class="increment" />
   </id>
   
   <property name="infor_name" column="infor_name" ></property>
   <property name="infor_description" column="infor_description" ></property>
   <property name="infor_flag" column="infor_flag"></property>
   <property name="infor_time" column="infor_time"></property>
   
 

</class>
</hibernate-mapping>